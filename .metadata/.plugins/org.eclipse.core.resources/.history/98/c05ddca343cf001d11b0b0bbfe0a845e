public class CalculadoraDeDescontos {
	
	public BigDecimal calcular(Orcamento orcamento) {
		BigDecimal desconto = new DescontoParaOrcamentoComMaisDeCincoItens(new DescontoParaOrcamentoComValorMaiorQueQuinhentos(proximo).calcular(orcamento));
		return BigDecimal.ZERO;
		
	}
}
